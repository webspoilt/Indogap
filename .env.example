# IndoGap Environment Configuration

# Application Settings
APP_ENV=development
DEBUG=true

# Database
# DATABASE_URL=postgresql://user:password@localhost:5432/indogap

# AI Provider Settings
# OLLAMA_BASE_URL=http://localhost:11434
# ENABLE_LOCAL_AI=true

# Resource Limits (for Local Ollama)
OLLAMA_NUM_PARALLEL=1      # One request at a time
OLLAMA_KEEP_ALIVE=300s     # Unload model after 5 minutes of inactivity
